{
  "name": "tokensuite-frontend-template",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "prettier": "prettier --write .",
    "svgr": "npx @svgr/cli -d src/components/atoms/icons --ignore-existing --icon --typescript public/icons",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "graphql:codegen": "graphql-codegen --config codegen.yml",
    "typechain": "typechain --target ethers-v5 --out-dir ./types/config/abis './src/config/abis/*.json'",
    "upgrade-latest": "yarn upgrade-interactive --latest"
  },
  "dependencies": {
    "@graphql-codegen/typescript-react-query": "^3.5.14",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/walletconnect-connector": "^6.2.13",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.3",
    "ethers": "^5.6.9",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0",
    "next": "12.1.6",
    "next-compose-plugins": "^2.2.1",
    "prettier": "^2.7.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "^3.39.1",
    "sass": "^1.52.3",
    "zustand": "^3.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.5.1",
    "@graphql-codegen/typescript-operations": "^2.4.2",
    "@svgr/cli": "^6.2.1",
    "@typechain/ethers-v5": "^8.0.5",
    "@types/node": "17.0.5",
    "@types/react": "17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-loader": "^8.2.5",
    "eslint": "^8.18.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "lint-prepush": "^2.2.1",
    "lint-staged": "13.0.2",
    "typechain": "^8.1.0",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint"
    ]
  },
  "lint-prepush": {
    "base": "dev",
    "tasks": {
      "*.{js,jsx,ts,tsx}": [
        "eslint"
      ]
    }
  }
}
